<!doctype html>
<html>
	<head>
		<script type="text/javascript" src="/bundles/fanferretquestion/js/require.js" data-main="/bundles/fanferretquestion/js/app.js"></script>
	</head>
	<body>
		{{form_start(form)}}
		{{form_end(form)}}
		{% for group in groups %}
		<h1>Group {{group.group.id}}</h1>
		{% for question in group.questions %}
		{{question.render()|raw}}
		{% endfor %}
		{% endfor %}
		<input type="button" onclick="document.getElementsByTagName('form')[0].submit();" value="Submit">
	</body>
</html>