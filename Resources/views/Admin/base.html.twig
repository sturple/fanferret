<!doctype html>
<html>
	<head>
		{% block head %}
		<meta charset="UTF-8">
		<title>{% block title %}{% endblock %}</title>
		<link rel="stylesheet" type="text/css" href="{{asset('bundles/fanferretquestion/css/bootstrap.min.css')|escape('html_attr')}}">
		<link rel="stylesheet" type="text/css" href="{{asset('bundles/fanferretquestion/css/font-awesome.min.css')|escape('html_attr')}}">
		<link rel="stylesheet" type="text/css" href="{{asset('bundles/fanferretquestion/css/admin.css')|escape('html_attr')}}">
		<script type="text/javascript" src="{{asset('bundles/fanferretquestion/js/jquery-3.1.0.min.js')|escape('html_attr')}}"></script>
		<script type="text/javascript" src="{{asset('bundles/fanferretquestion/js/bootstrap.min.js')|escape('html_attr')}}"></script>
		<script type="text/javascript" src="{{asset('bundles/fanferretquestion/js/require.js')|escape('html_attr')}}" data-main="{{asset('bundles/fanferretquestion/js/app.js')|escape('html_attr')}}"></script>
		{% endblock %}
	</head>
	<body>
		{% block body %}
		<div class="container">
			<img class="fanferret-logo" src="{{asset('bundles/fanferretquestion/images/fanferret-logo-white.png')|escape('html_attr')}}" alt="FanFerret Logo">
			{% block content %}{% endblock %}
			<div class="fanferret-admin-nav row">
				{% block nav %}
				<div class="col-md-4">
					<a class="btn btn-primary" href="{{path('fos_user_security_logout')|escape('html_attr')}}">Logout</a>
				</div>
				{% endblock %}
			</div>
		</div>
		{% endblock %}
	</body>
</html>